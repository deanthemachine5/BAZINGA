      <add url="http://services.dean.com/Email/MessageService.svc"/>
      <add url="https://www.google.com/" />
      <add url="https://yahoo.com/"/>
    </urls>
    <emails>
      <add address="deansouthern@gmail.com" />
    </emails>
    <errorCodes>
      <add code="501" />
      <add code="503"/>
    </errorCodes>
  </webMonitorConfig>
  
  
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <applicationSettings>

  </applicationSettings>
